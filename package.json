{
  "name": "vite-react-typescript-recoil-query",
  "version": "0.1.0",
  "author": "CozyFex <cozyfex@gmail.com>",
  "keywords": [
    "vite",
    "react",
    "typescript",
    "react-typescript",
    "recoil",
    "react-query"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cozyfex/vite-react-typescript-recoil-query.git"
  },
  "bugs": {
    "url": "https://github.com/cozyfex/vite-react-typescript-recoil-query/issues"
  },
  "homepage": "https://github.com/cozyfex/vite-react-typescript-recoil-query",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "dev:server": "nodemon -e ts,tsx --watch server --exec 'ts-node server/server.ts'",
    "dev:client": "vite --config vite.config.ts dev",
    "build": "rimraf dist && tsc -p tsconfig.prod.json && npm run build:client && npm run build:server && npm run copy-files",
    "build:client": "vite build --outDir dist --ssrManifest",
    "build:server": "vite build --ssr server/render.tsx --outDir dist/server",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "serve": "npm run build && cross-env NODE_ENV=production npm run ts-node server/server",
    "serve:local": "vite serve",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles static/* dist/assets && copyfiles index.html dist && copyfiles -f dist/assets/*"
  },
  "dependencies": {
    "@babel/core": "^7.19.6",
    "@babel/register": "^7.18.9",
    "axios": "^1.1.2",
    "babel-loader": "^8.2.5",
    "babel-preset-react-app": "^10.0.1",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hot-toast": "^2.4.0",
    "react-loader-spinner": "^5.3.4",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.5",
    "recoil-sync": "^0.2.0",
    "resolve": "^1.22.1",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^4.12.0",
    "@types/babel__core": "^7.1.19",
    "@types/babel__register": "^7.17.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.3",
    "@types/nodemon": "^1.19.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/resolve": "^1.20.2",
    "@types/rimraf": "^3.0.2",
    "@vitejs/plugin-react": "^2.0.0",
    "concurrently": "^7.4.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "jsdom": "^20.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
